#!/bin/sh
echo configure: creating wrapper Makefile for relsat

cat <<EOF > Makefile
OBJS=Clause.o ClauseList.o Debug.o RandGen.o Random.o SATInstance.o SATPreprocessor.o SATSolver.o VariableList.o 

.PHONY: all
all: relsat librelsat.a

librelsat.a: \$(OBJS)
	ar -q -v librelsat.a \$(OBJS)

include Makefile.linux
EOF

exit 0
